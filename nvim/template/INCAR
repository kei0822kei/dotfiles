      PREC = Accurate
    IBRION = 
       NSW = 
    NELMIN = 5
      ISIF = 3
     ENCUT = 
     EDIFF = 1.000000e-08
    EDIFFG = -1.000000e-08
    ISMEAR = 0
     SIGMA = 1.000000e-02
     IALGO = 38
     LREAL = .FALSE.
     LWAVE = .FALSE.
   ADDGRID = .TRUE.
    LCHARG = .FALSE.
      NPAR = 4



      PREC = Accurate       # 計算精度
    IBRION =                # relax --> 2  force --> -1 
       NSW =                # relax --> about40  force --> 0
    NELMIN = 5              # セルフコンシステントループの下限
      ISIF = 3              # 応力テンソルをどう計算するか、ion cell shape volume を緩和するかどうか
     ENCUT =                # POTCARのENMAXの大きいほうの1.3倍
     EDIFF = 1.000000e-08   # 電子の収束
    EDIFFG = -1.000000e-08  # イオンの収束
    ISMEAR = 0              # 波動関数の表現方法　0-> gaussian
     SIGMA = 1.000000e-02   # 多分gaussian　のシグマ
     IALGO = 38             # よくわからん、いじらんでいい
     LREAL = .FALSE.
     LWAVE = .FALSE.        # 波動関数をwavecarに書くか
    LCHARG = .FALSE.        # 電荷密度をchgcarに書くか
      NPAR = 4              # number of cores 計算機関係？


#  SYSTEM = Aluminum  # 1行目はコメント

#    PREC = Normal    # 計算精度を決める AccurateかNormalが良いらしい. default:Medium for VASP.4.X
#    ISIF = 3         # 応力テンソルの計算と，ion,cell shape,cell volumeを緩和をするかどうか　default:2
#  IBRION = 2         # 原子をどう動かすか. default:-1 or 0.
#                     # 構造緩和に利用する計算手法　2-> 共役勾配法
#   POTIM = 0.5       # default:0.5
#     NSW = 50        # ionic step の回数 default:0
#    ALGO = Normal    # electronic minimisationのアルゴリズムを決める. 他にFast,VeryFastなど default:N
#    NELM = 60        # electronic step の最大値. 60回まで行くとやり過ぎ. default:60
#  NELMIN = 2         # electronic step の最小値. 表面の計算では8にした方が良いらしい. default:2
#   ENCUT = 400       # 平面波を打ち切るエネルギー　default:ENMAX in POTCAR(PREC=A,Nなら)
# ADDGRID = .FALSE.   # The accuracy of forces can be further improved by specifying .TRUE.
#   EDIFF = 1.0e-05   # electronic selfconsistency-loop の収束条件. これぐらいで良いらしい. default:1.0e-4
#  EDIFFG = -0.02     # ionic relaxation loop の収束条件. default:EDIFF×10
#   ISPIN = 1         # スピンを考慮する(2)か,しない(1)か. default:1
#    ISYM = 2         # default:2 (対称性を考慮) 0で考慮しない. DOSを書くときに0にすると縮退した軌道の各成分がきちんと一致するらしい…．
# SYMPREC = 1.0e-05   # default:1.0e-05
#  ISMEAR = 0         # 金属：1(default)，半導体，絶縁体：0 atomの計算も0
#   SIGMA = 0.05      # 小さくすると必要なk点数が増える. が,大きいとギャップが埋まったりピークが曖昧になったり．SIGMA=0,k点数∞が理想らしい．default(0.2)
#   LREAL = Auto      # calcurate in real space or in reciplocal space(default:.False.) 
#  ISTART = 0         # 1,2はWAVECARを読み込んで計算. default:0. if WAVECAR excists, 1.
#  ICHARG = 2         # 'initial' charge densityをどうするか. この場合default:2. +10でnon-selfconsistent
#   LWAVE = .FALSE.   # WAVECARの出力 default:TRUE
#  LCHARG = .FALSE.   # CHGCARの出力 default:TRUE
#  LORBIT =           # RWIGS,PROCARどうするか. In vasp.4.6 the site projected DOS can be evaluated for LORBIT=10-12.
#  NBANDS =           # バンドの数 default:(NELECT+NIONS)/2 最低限1つ，空いたバンドが必要
#    NPAR =           # (the number of nodes working on one band) = (the total number of nodes)/NPAR
