Host vega
  User mizo
  Hostname vega
  IdentityFile ~/.ssh/id_rsa

Host github.com
  HostName github.com
  User git
  IdentityFile "/home/mizo/.ssh/id_git_rsa"

Host gitlab.com
  HostName gitlab.com
  User git
  IdentityFile "/home/mizo/.ssh/gitlab_rsa"

Host *
ServerAliveInterval 60
TCPKeepAlive yes
